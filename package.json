  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand",
    "test:e2e:watch": "jest --watch --config ./jest-e2e.json",
    "gen-env": "npx gen-env-types .env -o src/env.d.ts -e .",
    "db:diagram": "typeorm-uml ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^7.6.17",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.17",
    "argon2": "^0.27.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "nodemailer": "^6.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@swc/core": "^1.2.117",
    "@swc/jest": "^0.2.11",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.36",
    "@types/nodemailer": "^6.4.1",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-runtime": "^6.26.0",
    "esbuild": "^0.14.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^27.4.3",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
